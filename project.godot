; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Set_3.0"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[autoload]

global="*res://scripts/global.gd"
Transition="*res://Transition.tscn"

[display]

window/size/width=600
window/size/height=1024
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"
window/stretch/shrink="1"
window/handheld/emulate_touchscreen=true

[jarsinger]

"Default Clear Color"=false

[rendering]

quality/subsurface_scattering/quality=2
quality/subsurface_scattering/scale=1
environment/default_clear_color=Color( 1, 1, 1, 1 )
environment/default_environment="res://default_env.tres"
